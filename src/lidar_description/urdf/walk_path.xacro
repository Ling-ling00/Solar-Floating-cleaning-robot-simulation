  <!-- Rod1 is considered the primary reference link. -->
  <!-- SDF pose for Rod1: (-1.075, -0.2175, 0.15) -->
  <!-- <joint name="base_to_world" type="fixed">
  <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
