  <link name="plane"/>

  <joint name="base_to_Rod1" type="fixed">
    <parent link="plane"/>
    <child link="Rod1"/>
    <origin xyz="-1.075 -0.2175 0.15" rpy="0 0 0"/>
  </joint>

  <link name="Rod1">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.000104167" ixy="0" ixz="0" iyy="33.0009" iyz="0" izz="33.0009"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="19.9 0.025 0.025"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="19.9 0.025 0.025"/>
      </geometry>
    </collision>
  </link>

  <!-- Rod2 -->
  <!-- SDF pose: (-1.075, 0.2175, 0.15) 
       Relative to Rod1: dx=0, dy=0.435, dz=0 -->
  <joint name="RodtoRod" type="fixed">
    <parent link="Rod1"/>
    <child link="Rod2"/>
    <origin xyz="0 0.435 0" rpy="0 0 0"/>
  </joint>

  <link name="Rod2">
