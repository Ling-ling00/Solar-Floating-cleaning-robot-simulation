        <box size="19.9 0.025 0.025"/>
      </geometry>
    </collision>
  </link>

  <!-- Common inertial/visual/collision properties for pathways -->
  <xacro:macro name="pathway_link" params="link_name">
    <link name="${link_name}">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.1"/>
        <inertia ixx="0.00176417" ixy="0" ixz="0" iyy="0.00208417" iyz="0" izz="0.00384667"/>
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.5 0.46 0.01"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.5 0.46 0.01"/>
        </geometry>
      </collision>
    </link>
  </xacro:macro>

  <!-- Rod1 position: (-1.075, -0.2175, 0.15) -->
